{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["build/**", "dist/**"]
    },
    "typecheck": {
      "dependsOn": ["^typecheck"],
      "outputs": []
    },
    "validate": {
      "outputs": []
    },
    "render:docs": {
      "outputs": ["website/docs/registry/**"]
    },
    "render:build": {
      "outputs": ["build/registry-md/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
