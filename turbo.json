{
  "$schema": "https://turborepo.dev/schema.v2.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "types/**", ".astro/**", ".next/**", "!.next/cache/**"]
    },
    "typecheck": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "validate": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "render": {
      "dependsOn": ["^build"],
      "cache": false,
      "outputs": []
    },
    "render:build": {
      "dependsOn": ["^build"],
      "outputs": ["$TURBO_ROOT$/build/registry-md/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
